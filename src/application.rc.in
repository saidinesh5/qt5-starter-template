IDI_ICON1   ICON    DISCARDABLE "@APPLICATION_ICON@"

1 VERSIONINFO
FILEVERSION ${${PROJECT_NAME}_VERSION_MAJOR},${${PROJECT_NAME}_VERSION_MINOR},${${PROJECT_NAME}_VERSION_PATCH},0
PRODUCTVERSION ${${PROJECT_NAME}_VERSION_MAJOR},${${PROJECT_NAME}_VERSION_MINOR},${${PROJECT_NAME}_VERSION_PATCH},0
FILEOS 0x4
FILETYPE 0x0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "@APPLICATION_COMPANY_NAME@"
      VALUE "FileDescription", "@APPLICATION_DESCRIPTION@"
      VALUE "FileVersion", "${${PROJECT_NAME}_VERSION_STRING_FULL}"
      VALUE "InternalName", "@APPLICATION_NAME@"
      VALUE "LegalCopyright", "@APPLICATION_COPYRIGHT@"
      VALUE "OriginalFilename", "@PROJECT_NAME@.exe"
      VALUE "ProductName", "@APPLICATION_NAME@"
      VALUE "ProductVersion", "${${PROJECT_NAME}_VERSION_STRING_FULL}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END
